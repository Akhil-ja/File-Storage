# Port where the app runs locally
PORT=3000

# MongoDB local connection string (replace with your actual connection URI)
MONGO_URI=mongodb://localhost:27017/file-storage

# JWT secret key (replace with your own secure key)
JWT_SECRET=your_jwt_secret_here

# JWT token expiration durations
JWT_ACCESS_TOKEN_EXPIRATION=1h
JWT_REFRESH_TOKEN_EXPIRATION=7d

# AWS S3 bucket configuration
AWS_BUCKET_NAME=your-s3-bucket-name
AWS_BUCKET_REGION=your-aws-region
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key

# Client origin allowed for CORS
CLIENT_ORIGIN=http://localhost:3000
